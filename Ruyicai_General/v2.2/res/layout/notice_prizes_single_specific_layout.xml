<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="wrap_content"
	android:layout_height="wrap_content" android:paddingTop="5px"
	android:paddingBottom="5px">

	<TableLayout android:id="@+id/notice_pirzes_single_specific_ball_linearlayout"
		android:layout_marginTop="6dip" android:layout_alignParentLeft="true"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
	</TableLayout>
	<!-- zlm 8.5 code modify -->
	<RelativeLayout
		android:id="@+id/notice_pirzes_single_specific_finaldateandmoneysum_linearlayout"
		android:layout_below="@+id/notice_pirzes_single_specific_ball_linearlayout"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:layout_height="wrap_content"
			android:layout_width="wrap_content" android:textColor="@color/black"
			android:textSize="12dip" android:layout_alignParentLeft="true"
			android:id="@+id/notice_prizes_single_specific_finalPrizesDate_id">
		</TextView>

		<TextView android:id="@+id/notice_prizes_single_specific_totalSum_id"
			android:textColor="@color/black" android:layout_alignParentRight="true"
			android:textSize="12dip" android:layout_width="wrap_content"
			android:layout_height="wrap_content">
		</TextView>
	</RelativeLayout>

	<RelativeLayout
		android:id="@+id/notice_pirzes_single_specific_noticedateandissue_linearlayout"
		android:layout_below="@+id/notice_pirzes_single_specific_finaldateandmoneysum_linearlayout"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_alignParentLeft="true"
			android:textColor="@color/black" android:textSize="12dip"
			android:id="@+id/notice_prizes_single_specific_noticedDate_id">
		</TextView>

		<TextView android:id="@+id/notice_prizes_single_specific_issue_id"
			android:textColor="@color/black" android:layout_alignParentRight="true"
			android:layout_marginRight="10px" android:textSize="12dip"
			android:layout_width="100dp" android:layout_height="wrap_content">
		</TextView>
	</RelativeLayout>

</RelativeLayout>
