<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://www.ruyicai.com/res/custom"
    android:layout_width="300dip"
    android:layout_height="wrap_content"
    android:background="@drawable/jc_bf_bg"
    android:orientation="vertical" >

    <ScrollView
        android:layout_width="300dip"
        android:layout_height="wrap_content"
        android:paddingBottom="60dip"
        android:paddingTop="55dip"
        android:scrollbars="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/linearLayout1"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal" >

                    <com.ruyicai.component.custom.checkbox.MyCheckBox
                        android:id="@+id/lq_sfc_dialog_check01"
                        android:layout_width="140dip"
                        android:layout_height="40dip"
                        android:background="@drawable/hun_btn_left"
                        android:button="@null"
                        custom:check_title="@string/jclq_sfc_dialog_title01" />

                    <com.ruyicai.component.custom.checkbox.MyCheckBox
                        android:id="@+id/lq_sfc_dialog_check02"
                        android:layout_width="140dip"
                        android:layout_height="40dip"
                        android:background="@drawable/hun_btn_right"
                        android:button="@null"
                        custom:check_title="@string/jclq_sfc_dialog_title03" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout2"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:paddingTop="5dip" >

                    <com.ruyicai.component.custom.checkbox.MyCheckBox
                        android:id="@+id/lq_sfc_dialog_check03"
                        android:layout_width="93dip"
                        android:layout_height="40dip"
                        android:background="@drawable/hun_btn_left"
                        android:button="@null"
                        custom:check_title="@string/jclq_sfc_dialog_title01" />

                    <com.ruyicai.component.custom.checkbox.MyCheckBox
                        android:id="@+id/lq_sfc_dialog_check04"
                        android:layout_width="94dip"
                        android:layout_height="40dip"
                        android:background="@drawable/jc_lq_hun_rfsf_dxf_center_bg"
                        android:button="@null"
                        custom:check_title="@string/jclq_sfc_dialog_title02" />

                    <com.ruyicai.component.custom.checkbox.MyCheckBox
                        android:id="@+id/lq_sfc_dialog_check05"
                        android:layout_width="93dip"
                        android:layout_height="40dip"
                        android:background="@drawable/hun_btn_right"
                        android:button="@null"
                        custom:check_title="@string/jclq_sfc_dialog_title03" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout3"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:paddingTop="5dip" >

                    <com.ruyicai.component.custom.checkbox.MyCheckBox
                        android:id="@+id/lq_sfc_dialog_check06"
                        android:layout_width="93dip"
                        android:layout_height="40dip"
                        android:background="@drawable/jc_lq_hun_dxf_left_selector"
                        android:button="@null"
                        android:visibility="gone"
                        custom:check_title="@string/jclq_sfc_dialog_title01" />

                    <com.ruyicai.component.custom.checkbox.MyCheckBox
                        android:id="@+id/lq_sfc_dialog_check07"
                        android:layout_width="94dip"
                        android:layout_height="40dip"
                        android:background="@drawable/jc_lq_hun_rfsf_dxf_center_bg"
                        android:button="@null"
                        android:visibility="gone"
                        custom:check_title="@string/jclq_sfc_dialog_title02" />

                    <com.ruyicai.component.custom.checkbox.MyCheckBox
                        android:id="@+id/lq_sfc_dialog_check08"
                        android:layout_width="93dip"
                        android:layout_height="40dip"
                        android:background="@drawable/jc_lq_hun_dxf_right_selector"
                        android:button="@null"
                        android:visibility="gone"
                        custom:check_title="@string/jclq_sfc_dialog_title03" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayout4"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:paddingTop="5dip" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check09"
                            android:layout_width="93dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_left_up"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title01" />

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check010"
                            android:layout_width="94dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_center_two"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title02" />

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check011"
                            android:layout_width="93dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_right_up"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title03" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check012"
                            android:layout_width="93dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_center_two"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title01" />

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check013"
                            android:layout_width="94dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_center_two"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title02" />

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check014"
                            android:layout_width="93dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_center_two"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title03" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check015"
                            android:layout_width="93dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_center_two"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title01" />

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check016"
                            android:layout_width="94dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_center_two"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title02" />

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check017"
                            android:layout_width="93dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_center_two"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title03" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check018"
                            android:layout_width="93dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_left_down"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title01" />

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check019"
                            android:layout_width="94dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_center_two"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title02" />

                        <com.ruyicai.component.custom.checkbox.MyCheckBox
                            android:id="@+id/lq_sfc_dialog_check020"
                            android:layout_width="93dip"
                            android:layout_height="50dip"
                            android:background="@drawable/hun_btn_right_down"
                            android:button="@null"
                            android:visibility="gone"
                            custom:check_title="@string/jclq_sfc_dialog_title03" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/layout_main_text_title"
            android:layout_width="match_parent"
            android:layout_height="45dip"
            android:gravity="center"
            android:textColor="@color/jc_main_play_title_color"
            android:textSize="18sp" />

        <View
            android:layout_width="match_parent"
            android:layout_height="1.5dip"
            android:layout_marginLeft="50dip"
            android:layout_marginRight="50dip"
            android:background="@color/zc_sfc_divider" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="300dip"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginTop="5dip"
        android:orientation="horizontal"
        android:padding="10dip" >

        <Button
            android:id="@+id/ok"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:background="@drawable/loginselecter"
            android:gravity="center"
            android:text="确 定"
            android:textColor="@color/white"
            android:textSize="18sp" >
        </Button>

        <Button
            android:id="@+id/canel"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:background="@drawable/loginselecter"
            android:gravity="center"
            android:text="取 消"
            android:textColor="@color/white"
            android:textSize="18sp" >
        </Button>
    </LinearLayout>

</FrameLayout>