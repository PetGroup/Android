<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://www.ruyicai.com/res/custom"
    android:layout_width="300dip"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/main_buy_title"
        android:layout_width="300dip"
        android:layout_height="wrap_content"
        android:background="@drawable/shouye_top" >

        <TextView
            android:id="@+id/layout_main_text_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text=""
            android:textColor="@color/white"
            android:textSize="20dip" >
        </TextView>
    </RelativeLayout>

    <ScrollView
        android:layout_width="300dip"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:paddingBottom="10dip"
        android:paddingTop="10dip"
        android:scrollbars="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="10dip" >

                <LinearLayout
                    android:id="@+id/linearLayout1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <RelativeLayout
                        android:id="@+id/linearLayout1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="10dip" >

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check01"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title01" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check02"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title02" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check03"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title03" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/linearLayout1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="10dip" >

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check04"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title04" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check05"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title05" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check06"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title06" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/linearLayout1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="10dip" >

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check07"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title11" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check08"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title12" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check09"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title13" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/linearLayout1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="10dip" >

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check010"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title14" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check011"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title15" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true" >

                            <com.ruyicai.custom.checkbox.MyCheckBox
                                android:id="@+id/lq_sfc_dialog_check012"
                                android:layout_width="80dip"
                                android:layout_height="wrap_content"
                                android:button="@drawable/check_lq_select"
                                custom:check_title="@string/jclq_sfc_dialog_title16" />
                        </RelativeLayout>
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:layout_width="300dip"
        android:layout_height="wrap_content"
        android:background="@drawable/alert_bottom_bg"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:padding="10dip" >

        <Button
            android:id="@+id/ok"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_weight="1"
            android:background="@drawable/join_info_btn_selecter"
            android:gravity="center"
            android:text="确 定"
            android:textColor="@color/black"
            android:textSize="18sp" >
        </Button>

        <Button
            android:id="@+id/canel"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:background="@drawable/join_info_btn_selecter"
            android:gravity="center"
            android:text="取 消"
            android:textColor="@color/black"
            android:textSize="18sp" >
        </Button>
    </LinearLayout>

</LinearLayout>