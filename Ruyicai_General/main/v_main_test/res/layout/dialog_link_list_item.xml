<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:background="@color/white">
	<RelativeLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<TextView android:text="" android:id="@id/dialog_link_item_text_title"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:textColor="@color/black" android:textSize="15sp"
			android:padding="10sp" android:layout_alignParentLeft="true"></TextView>
		<TextView android:text="" android:id="@id/dialog_link_item_text_phone"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:textColor="@color/black" android:textSize="14sp"
			android:padding="10sp" android:layout_toLeftOf="@id/dialog_link_item_check" android:paddingRight="15sp"></TextView>
		<CheckBox android:layout_width="wrap_content" android:text=""
			android:id="@id/dialog_link_item_check" android:layout_height="wrap_content"
			android:layout_alignParentRight="true" android:paddingRight="10sp"></CheckBox>
	</RelativeLayout>
</LinearLayout>
