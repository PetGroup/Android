<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@color/list_color">
	<RelativeLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:background="@drawable/shouye_top" android:id="@+id/ruyihelper_listview_relative"
		android:paddingLeft="10sp">
		<Button android:id="@+id/ruyizhushou_btn_return"
			android:layout_width="70dp" android:layout_height="30dp"
			android:layout_alignParentRight="true" android:layout_marginRight="10dip"
			android:layout_centerVertical="true" android:text="@string/str_return"
			android:textColor="@color/white"/>
		<TextView android:id="@+id/ruyipackage_title"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_centerHorizontal="true" android:textAppearance="?android:attr/textAppearanceMedium"
			android:typeface="serif" android:text="@string/bangzhuzhongxin"
			android:textColor="@color/white" android:textSize="20sp"
			android:layout_centerVertical="true" />
	</RelativeLayout>
	<ListView android:id="@+id/ruyihelper_listview_ruyihelper_id"
		android:cacheColorHint="#00000000" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:background="@color/white">
	</ListView>
</LinearLayout>