<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:orientation="vertical" android:background="#FFFFFF">
	<LinearLayout android:id="@+id/danshi_balls_layout"
		android:orientation="vertical" android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<RadioGroup android:id="@+id/radiogroupid"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:orientation="vertical">
			<RadioButton android:id="@+id/radio_zu3_danshi"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:text="@string/danshi" android:textColor="#000000"
				android:checked="true" />
			<TableLayout android:id="@+id/table_zu3_danshi_baiwei"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_marginRight="6dip" />
			<TableLayout android:id="@+id/table_zu3_danshi_shiwei"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_marginRight="6dip" />
			<TableLayout android:id="@+id/table_zu3_danshi_gewei"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_marginRight="6dip" />
			<RadioButton android:id="@+id/radio_zu3_fushi"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:text="@string/fushi" android:textColor="#000000"
				android:checked="false" />
			<TableLayout android:id="@+id/table_zu3_fushi"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:layout_marginRight="6dip" />
		</RadioGroup>
	</LinearLayout>

	<TextView android:id="@+id/text_sum_money_zu3"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:textColor="#000000" android:paddingLeft="10px" />



	<LinearLayout android:orientation="vertical"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:paddingLeft="10px">


		<TextView android:id="@+id/fc3d_beishu_title"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:text="@string/meiqibeishu" android:textColor="#000000" />

		<RelativeLayout android:id="@+id/fc3d_jixuan_layout"
			android:layout_width="300px" android:layout_height="wrap_content">

			<TextView android:id="@+id/text_beishu"
				android:layout_width="30px" android:layout_height="wrap_content"
				android:layout_marginLeft="10dip" android:layout_marginRight="5dip"
				android:textSize="20px" android:textColor="#000000"
				android:layout_alignParentLeft="true" />

			<ImageButton android:id="@+id/fc3d_seekbar_subtract_zu3_beishu"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:src="@drawable/subtract11" android:layout_toRightOf="@+id/text_beishu"
				android:background="#00000000" />


			<SeekBar android:id="@+id/seek_beishu"
				style="?android:attr/progressBarStyleHorizontal"
				android:progressDrawable="@drawable/seekbar_style" android:thumb="@drawable/thumb"
				android:max="50" android:progress="1" android:layout_width="180px"
				android:layout_height="23dip" android:paddingLeft="15dip"
				android:paddingRight="15dip" android:paddingBottom="4dip"
				android:layout_toRightOf="@+id/fc3d_seekbar_subtract_zu3_beishu" />

			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="@string/zuida50bei"
				android:textColor="#000000" android:layout_below="@+id/seek_beishu"
				android:layout_centerHorizontal="true" />

			<ImageButton android:id="@+id/fc3d_seekbar_add_zu3_beishu"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:src="@drawable/add11" android:layout_toRightOf="@+id/seek_beishu"
				android:background="#00000000" android:layout_marginRight="10dip" />

		</RelativeLayout>


	</LinearLayout>



	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:visibility="gone">

		<TextView android:id="@+id/text_qishu_title"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:text="@string/zhuihaoqishu" android:textColor="#000000" />

		<RelativeLayout android:id="@+id/fc3d_jixuan_layout_qihao"
			android:layout_width="300px" android:layout_height="wrap_content">
			<TextView android:id="@+id/text_qishu" android:layout_width="30px"
				android:layout_height="wrap_content" android:textColor="#000000"
				android:layout_marginLeft="10dip" android:layout_marginRight="5dip"
				android:layout_alignParentLeft="true" android:textSize="20px" />

			<ImageButton android:id="@+id/fc3d_seekbar_subtract_zu3_qihao"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:src="@drawable/subtract11" android:layout_toRightOf="@+id/text_qishu"
				android:background="#00000000" />


			<SeekBar android:id="@+id/seek_qishu"
				style="?android:attr/progressBarStyleHorizontal"
				android:progressDrawable="@drawable/seekbar_style" android:thumb="@drawable/thumb"
				android:max="99" android:progress="1" android:layout_width="180px"
				android:layout_height="23dip" android:paddingLeft="15dip"
				android:paddingRight="15dip" android:paddingBottom="4dip"
				android:layout_toRightOf="@+id/fc3d_seekbar_subtract_zu3_qihao" />

			<TextView android:layout_width="wrap_content"
				android:layout_height="wrap_content" android:text="@string/zuiduo99qi"
				android:textColor="#000000" android:layout_below="@+id/seek_qishu"
				android:layout_centerHorizontal="true" />

			<ImageButton android:id="@+id/fc3d_seekbar_add_zu3_qihao"
				android:layout_width="wrap_content" android:layout_height="wrap_content"
				android:src="@drawable/add11" android:layout_toRightOf="@+id/seek_qishu"
				android:background="#00000000" android:layout_marginRight="10dip" />

		</RelativeLayout>
	</LinearLayout>


	<RelativeLayout android:id="@+id/fc3d_jixuan_layout"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:paddingLeft="10px">

		<CheckBox android:id="@+id/cb_jixuan_zu3"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:text="@string/jixuan" android:layout_alignParentLeft="true"
			android:textColor="#000000" />
		<ImageButton android:layout_height="wrap_content"
			android:layout_below="@+id/cb_jixuan_zu3" android:layout_width="wrap_content"
			android:id="@+id/ssq_join_fushi_button_new" android:background="@drawable/add_touzhu"></ImageButton>
		<Button android:id="@+id/fc3d_zu3_newselected_btn"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:text="@string/ruyichuanqing_newselect_jixuan"
			android:layout_toRightOf="@+id/cb_jixuan_zu3" android:visibility="invisible" 
			android:paddingRight="5dp"/>
		<ImageButton android:id="@+id/b_touzhu_zu3"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:background="#00000000" android:src="@drawable/imageselecter"
			android:layout_alignParentRight="true" />

	</RelativeLayout>

	<ListView android:layout_height="wrap_content"
		android:id="@+id/ssq_join_fushi_list_fanan" android:layout_width="fill_parent"
		android:scrollbars="none"></ListView>
</LinearLayout>