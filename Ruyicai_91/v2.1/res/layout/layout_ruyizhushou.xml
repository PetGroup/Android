<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@color/white">
	<RelativeLayout android:orientation="horizontal"
		android:gravity="left" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:background="@drawable/bottom_bar"
		android:paddingTop="10px" android:paddingLeft="10px">

		<ImageView android:id="@+id/ruyizhushou_btn_return"
			android:background="#00000000" android:src="@drawable/return_btn"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_alignParentLeft="true" />
		<TextView android:id="@+id/ruyipackage_title"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:gravity="center" android:textAppearance="?android:attr/textAppearanceMedium"
			android:typeface="serif" android:text="" android:layout_marginRight="10px"
			android:textColor="@color/white" android:textSize="20px" />
	</RelativeLayout>
	<WebView android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/ruyipackage_webview" />
</LinearLayout>
