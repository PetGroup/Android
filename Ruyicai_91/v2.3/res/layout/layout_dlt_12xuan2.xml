<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:orientation="vertical">
	<TextView android:id="@+id/dlt_twoin12_text_sum_money"
		android:text="@string/please_choose_number" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:textColor="#000000"
		android:layout_gravity="center_horizontal" />
	<LinearLayout android:layout_width="fill_parent"
		android:background="@drawable/edit_area" android:layout_height="wrap_content"
		android:orientation="horizontal">
		<LinearLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:orientation="horizontal"
			android:paddingTop="20dp" android:paddingLeft="20dp"
			android:paddingBottom="20dp" android:paddingRight="15dp">
			<TextView android:text="@string/zi_xuanzedezhuma"
				android:id="@+id/dlt_new_front_dan_text" android:layout_width="50dp"
				android:layout_height="wrap_content" android:layout_weight="1"
				android:gravity="center_vertical" android:textColor="@color/black" />
			<EditText android:hint="@string/dlt_12xuan2_edit_text"
				android:layout_weight="3" android:id="@+id/dlt_12xuan2_edit_text"
				android:layout_width="300dp" android:layout_height="wrap_content"
				android:singleLine="true" android:cursorVisible="false"
				android:textSize="13sp" android:editable="false" android:background="@drawable/huikuang"
				android:textColor="@color/red" />
		</LinearLayout>
	</LinearLayout>
	<RelativeLayout android:id="@+id/RelativeLayout01"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
		<ScrollView android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:paddingBottom="85dip">
			<LinearLayout android:layout_width="fill_parent"
				android:id="@+id/bujuscroll" android:background="@drawable/balls_area"
				android:layout_height="wrap_content" android:orientation="vertical">
				<LinearLayout android:layout_width="fill_parent"
					android:layout_height="wrap_content" android:orientation="vertical"
					android:id="@+id/ssq_zhixuan_linear_red">
					<TableLayout android:id="@+id/table_blue"
						android:layout_width="fill_parent" android:layout_height="wrap_content"
						android:layout_marginRight="6dip" android:layout_marginTop="10dip"
						android:layout_marginBottom="10dip" />
				</LinearLayout>

			</LinearLayout>

		</ScrollView>
		<RelativeLayout android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:layout_alignParentBottom="true" android:background="@drawable/bottom_bg">
			<RelativeLayout android:orientation="horizontal"
				android:id="@+id/layout_togglebutton" android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerHorizontal="true" android:paddingBottom="10dip">

				<LinearLayout android:orientation="vertical"
					android:layout_width="wrap_content" android:layout_height="wrap_content"
					android:paddingRight="20dip" android:id="@+id/layout_progress_all">
					<LinearLayout android:id="@+id/ssq_jixuan_layout"
						android:layout_width="wrap_content" android:layout_height="wrap_content">
						<ImageButton android:id="@+id/dlt_twoin12_seekbar_subtract_beishu"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:src="@drawable/subtract11" android:background="#00000000"
							android:paddingTop="10dip" />
						<LinearLayout android:orientation="vertical"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:id="@+id/layout_progress"
							android:layout_centerHorizontal="true"
							layout_toRightOf="@+id/dlt_zhixuan_seekbar_subtract_beishu">
							<LinearLayout android:orientation="horizontal"
								android:layout_width="wrap_content" android:layout_height="wrap_content"
								android:layout_gravity="center_horizontal">
								<TextView android:id="@+id/dlt_twoin12_text_beishu"
									android:layout_width="20dip" android:layout_height="wrap_content"
									android:textColor="@color/black" />
								<TextView android:id="@+id/dlt_twoin12_beishu_title"
									android:layout_width="wrap_content" android:layout_height="wrap_content"
									android:text="@string/meiqibeishu" android:textColor="@color/black" />
							</LinearLayout>
							<SeekBar android:id="@+id/dlt_twoin12_seek_beishu"
								style="?android:attr/progressBarStyleHorizontal"
								android:progressDrawable="@drawable/seekbar_style"
								android:thumb="@drawable/thumb" android:max="99"
								android:progress="1" android:layout_width="130dip"
								android:layout_height="15dip" android:paddingLeft="15dip"
								android:paddingRight="15dip" android:paddingBottom="4dip"

								android:layout_centerVertical="true" />
						</LinearLayout>
						<ImageButton android:id="@+id/dlt_twoin12_seekbar_add_beishu"
							android:layout_marginRight="20dp" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:src="@drawable/add11"
							android:layout_toRightOf="@+id/layout_progress"
							android:background="#00000000" android:paddingTop="10dip" />
						<ImageButton android:id="@+id/dlt_twoin12_b_touzhu"
							android:layout_toRightOf="@+id/dlt_zhixuan_seekbar_add_beishu"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:background="#00000000" android:src="@drawable/imageselecter"
							android:layout_gravity="bottom" />
					</LinearLayout>
					<RelativeLayout android:id="@+id/ssq_jixuan_layout_qihao"
						android:layout_width="wrap_content" android:layout_height="wrap_content">
						<ImageButton android:id="@+id/dlt_twoin12_seekbar_subtract_qihao"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:src="@drawable/subtract11" android:background="#00000000"
							android:paddingTop="10dip" />
						<LinearLayout android:orientation="vertical"
							android:layout_width="wrap_content" android:layout_height="40dip"
							android:id="@+id/layout_progress"
							android:layout_centerHorizontal="true"
							android:layout_toRightOf="@+id/dlt_twoin12_seekbar_subtract_qihao">
							<LinearLayout android:orientation="horizontal"
								android:layout_width="wrap_content" android:layout_height="wrap_content"
								android:layout_gravity="center_horizontal">
								<TextView android:id="@+id/dlt_twoin12_text_qishu"
									android:layout_width="20dip" android:layout_height="wrap_content"
									android:textColor="#000000" />
								<TextView android:id="@+id/dlt_twoin12_text_qishu_title"
									android:layout_width="wrap_content" android:layout_height="wrap_content"
									android:text="@string/zhuihaoqishu" android:textColor="#000000" />
							</LinearLayout>
							<SeekBar android:id="@+id/dlt_twoin12_seek_qishu"
								style="?android:attr/progressBarStyleHorizontal"
								android:progressDrawable="@drawable/seekbar_style"
								android:thumb="@drawable/thumb" android:max="99"
								android:progress="1" android:layout_width="130dip"
								android:layout_height="15dip" android:paddingLeft="15dip"
								android:paddingRight="15dip" android:paddingBottom="4dip"
								android:layout_gravity="center_horizontal"
								android:layout_centerVertical="true" />
						</LinearLayout>
						<ImageButton android:id="@+id/dlt_twoin12_seekbar_add_qihao"
							android:layout_marginRight="20dp" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:src="@drawable/add11"
							android:layout_toRightOf="@+id/layout_progress"
							android:background="#00000000" android:paddingTop="10dip" />
						<ImageButton android:id="@+id/dlt_12xuan2_btn_newselect"
							android:layout_toRightOf="@+id/dlt_twoin12_seekbar_add_qihao"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:background="#00000000" android:src="@drawable/selecet_chongxuan"
							android:paddingTop="10dip" android:layout_centerVertical="true" />
					</RelativeLayout>
				</LinearLayout>
			</RelativeLayout>
		</RelativeLayout>
	</RelativeLayout>
</LinearLayout>